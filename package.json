{
  "name": "Prodex",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --host 0.0.0.0",
    "build:dev": "vue-cli-service build --mode development",
    "lint": "vue-cli-service lint --fix",
    "start": "npm run serve",
    "analyzer": "use_analyzer=true npm run serve",
    "build:prod": "vue-cli-service build --mode production",
    "serve:prod": "vue-cli-service serve --host 0.0.0.0 --mode production"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "clipboard": "^2.0.6",
    "compression-webpack-plugin": "^5.0.2",
    "element-ui": "^2.14.1",
    "fastclick": "^1.0.6",
    "less-loader": "^5.0.0",
    "sass": "^1.29.0",
    "sass-loader": "^10.1.0",
    "scss": "^0.2.4",
    "vue": "^2.6.11",
    "vue-router": "^3.4.9",
    "vuex": "^3.5.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-component": "^1.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "less": "^4.1.1",
    "terser-webpack-plugin": "^4.2.3",
    "vue-i18n": "^8.22.2",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "generator-star-spacing": "off",
      "no-tabs": "off",
      "no-unused-vars": "off",
      "no-console": "off",
      "no-irregular-whitespace": "off",
      "no-debugger": "off",
      "eqeqeq": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "description": "```\r yarn install\r ```",
  "main": ".eslintrc.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/foxdex/dex-com.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/foxdex/dex-com/issues"
  },
  "homepage": "https://github.com/foxdex/dex-com#readme"
}
